<Window x:Class="Uppgift2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift2"
        mc:Ignorable="d"
        Title="Ditt namn här" Height="276" Width="357">
    <Grid Margin="0,10,0,-6">
        <TextBox x:Name="txtName" HorizontalAlignment="Left" Margin="26,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="125" Height="35" FontSize="16" FontFamily="Candara" FontWeight="Bold"/>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="26,75,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.715,-0.012" Height="35" Width="68" Click="OK_Click"/>
        <Label x:Name="lblName" Content="Namn" HorizontalAlignment="Left" Margin="26,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
